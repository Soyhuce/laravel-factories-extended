{
    "name": "soyhuce/laravel-factories-extended",
    "description": "Extensions for Laravel model factories",
    "keywords": [
        "laravel",
        "eloquent",
        "factory"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bastien Philippe",
            "email": "bastien.philippe@soyhuce.fr"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "soyhuce/next-ide-helper": "^0.3.0"
    },
    "suggest": {
        "soyhuce/next-ide-helper": "Laravel ide helper rebuilt under steroids, to document Factory extensions"
    },
    "autoload": {
        "psr-4": {
            "Soyhuce\\LaravelFactoriesExtended\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soyhuce\\LaravelFactoriesExtended\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
